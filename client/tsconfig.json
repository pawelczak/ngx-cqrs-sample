{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"baseUrl": ".",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"removeComments": false,
		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"skipLibCheck": true,
		"typeRoots": [
			"./node_modules/@types"
		],
		"lib": [
			"dom",
			"es2015",
			"es2015.iterable"
		],
		"types": [
			"es6-shim",
			"jasmine",
			"node"
		],
		"paths": {
			"ngx-cqrs/*": [ "src/util/ngx-cqrs/*" ],
			"ngx-cqrs": [ "src/util/ngx-cqrs" ]
		}
	},
	"exclude": [
		"node_modules",
		"*.json"
	],
	"awesomeTypescriptLoaderOptions": {
		"useWebpackText": true
	},
	"compileOnSave": false,
	"buildOnSave": false
}
